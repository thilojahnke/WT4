{
	"_version": "1.0.0",
	"sap.app": {
		"_version": "1.0.0",
		"id": "WT4",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"titel": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"adressRemote": {
				"uri": "/proxy/AdressODATA/Adress.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.0.0",
		"technologie": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supported themes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.0.0",
		"rootView": "WT4.view.AdressApp",
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {
					
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "WT4.i18n.i18n"
				}
			},
			"adr": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oAdr": {
				"dataSource": "adressRemote"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "WT4.view",
				"controlId": "adressapp",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "address",
					"target": "address"
				},
				{
					"pattern": "detail",
					"name": "detail",
					"target": "detail"
				},
				{
					"pattern": "add",
					"name": "add",
					"target": "add"
				}
			],
			"targets": {
				"address": {
					"viewId": "address",
					"viewName": "address"
				},
				"detail": {
					"viewId": "detail",
					"viewName": "Detail"
				},
				"add": {
					"viewId": "add",
					"viewName": "AddAdress"
				}
			}
		}
	}
}